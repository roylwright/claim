package w1p4_input;

import java.util.Scanner;

public class Input {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("Enter a value between 0 and 100: ");
		int userInput = scanner.nextInt();
		System.out.print(userInput);
		scanner.close();

	}

}
