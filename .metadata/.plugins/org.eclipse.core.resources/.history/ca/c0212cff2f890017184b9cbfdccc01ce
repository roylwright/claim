package w1aWTF_loopy;

public class Loopy {

	public static void main(String[] args) {
		int j = 0;
		int k = 0;
		int l = 0;
		for(int i=0; i<11; i++) {
			j=j+i;
		}
		System.out.println(j);
		for(int i=20; i<31; i++) {
			k=k+i;
		}
		System.out.println(j);
		for(int i=35; i<46; i++) {
			l=l+i;
		}
		System.out.println(j);
	}

}
