package w1a1_XOR;

public class XOR {

	public static void main(String[] args) {
		boolean one = 'true';
		boolean two = 'false';
		xOR();
	}
	public static void xOR(boolean one, boolean two) {
		if (one != two) {
			System.out.print("The statement is true");
		} else {
			System.out.print("The statement is false");
		}
	}
}
